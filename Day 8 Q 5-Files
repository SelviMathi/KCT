#include <fstream>
#include <iostream>
#include <iomanip>

using namespace std;

int main() {
    ofstream outfile;
    outfile.open("ages.txt");

    int n;
    cin >> n;

    if (n > 20) {
        cout << "Exceeding limit!";
        return 0;
    }
    else{
        for (int i = 0; i < n; i++) {
            int age;
            cin >> age;
            outfile << age << " ";
        }
    }
    outfile.close();

    ifstream infile;
    infile.open("ages.txt");

    int age;
    float sum = 0;
    float average;
    while (infile >> age) {
        sum += age;
        average = sum / n;
    }  
    cout << fixed << setprecision(1) << average << endl;   

    infile.close();
    return 0;
}
