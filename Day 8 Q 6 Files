#include <fstream>
#include <iostream>
using namespace std;

int main() {
    ofstream fout;
    fout.open("numbers.txt");

    int n;
    cin >> n;

    for (int i = 0; i < n; i++) {
        int num;
        cin >> num;
        fout << num << " ";
    }
fout.close();

    ifstream fin;
    fin.open("numbers.txt");

    int num,evenCount = 0,oddCount = 0;
    while (fin >> num) {
        if (num % 2 == 0) {
            evenCount++;
        } 
        else {
            oddCount++;
        }
    }

    cout << "Even: " << evenCount << endl<<"Odd: " << oddCount << endl;

    fin.close();
    return 0;
}
