#include <iostream>
#include <cmath>
using namespace std;

bool function(int a)
{
   int sum=0;
   for(int i=1;i<=a/2;i++)
   {
       if(a%i==0)sum+=i;
   }
       return sum==a;
}

int main() {
    int x;
    cin >> x;
     cout<<(function(x)?"Perfect":"Not perfect");
    return 0;
}
