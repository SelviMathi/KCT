#include<iostream>
using  namespace std;

int main()
{
    string str;
    cin>>str;
    int l=0,r=str.length()-1;
    while(l<r)
    {
        if(str[l]!=str[r])
        {
            cout<<str<<" is not a palindrome";
            return 0;
        }
        l++;
        r--;
    }
    cout<<str<<" is a palindrome";
}
